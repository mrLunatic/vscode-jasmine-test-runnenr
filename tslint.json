{
    "extends": "tslint:recommended",
    "rules": {
        "no-string-throw": true,
        "no-unused-expression": true,
        "no-duplicate-variable": true,
        "curly": true,
        "class-name": true,
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": true,
        "interface-name" : [
            true,
            "never-prefix"
        ]
    },
    "defaultSeverity": "warning"
}
