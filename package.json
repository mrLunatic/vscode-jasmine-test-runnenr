{
    "name": "vscode-jasmine-test-runner",
    "displayName": "Jasmine test runner",
    "description": "Toot to run and debug jasmine tests",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.37.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:jasmine-ts.run-test"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "vscode-jasmine-test-runner.debug-test",
                "title": "Jasmine test: Debug"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.37.0",
        "typescript": "^3.3.1",
        "tslint": "^5.12.1"
    }
}
